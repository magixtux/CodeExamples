@model WeatherServiceConsumption.Models.WeatherInfo
@{
    ViewBag.Title = "Index";
}

<h2>Index - @ViewBag.Message</h2>
<div class="row">
    <div class="col-md-4"></div>
    <div class="col-md-4">
        <p>
            Looking at the current weather for @Model.Name<br/>
            The temperature is @(Model.Main.Temp)C with a pressure of @Model.Main.Pressure <br/>
            Coordinates for area is: @Model.Coord.Lat , @Model.Coord.Lon

        </p>


    </div>
    <div class="col-md-4"></div>
    
    
</div>